    "custom/toggle-mic": {
        "format": "{}",
        "interval": "once",
        "exec": "pactl get-source-mute @DEFAULT_SOURCE@ | grep 'Mute: yes' >/dev/null && echo 'ÔÑ±' || echo 'ÔÑ∞'",
        "on-click": "pactl set-source-mute @DEFAULT_SOURCE@ toggle && pkill -RTMIN+4 waybar",
        "tooltip": "false",
        "signal": 4
    },
    "custom/toggle-cam": {
        "format": "{}",
