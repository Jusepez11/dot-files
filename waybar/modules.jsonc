        "exec": "modprobe uvcvideo && echo 'Û∞ÑÄ' || echo 'Û∞óü'", 
        "on-click": "pkexec modprobe -r uvcvideo || zenity --password | sudo -S modprobe uvcvideo; pkill -RTMIN+4 waybar",
        "tooltip": "false",
        "signal": 4
    },
